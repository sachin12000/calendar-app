{
  "name": "fitness-tracker",
  "version": "1.0.0",
  "description": "Fitness tracker built with React, Material Design and Express.",
  "main": "index.js",
  "scripts": {
    "emulators": "cd ./src/server && firebase emulators:start --import=dummy_data --only=auth,firestore",
    "client": "npx webpack-dev-server --config webpack.config.js --mode development",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "deploy:hosting": "cd ./src/server && firebase deploy --only hosting",
    "deploy:firestore": "cd ./src/server && firebase deploy --only firestore:rules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sachin12000/fitness-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sachin12000/fitness-tracker/issues"
  },
  "homepage": "https://github.com/sachin12000/fitness-tracker#readme",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase": "^10.1.0",
    "graphql": "^15.8.0",
    "graphql-http": "^1.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^2.0.22",
    "process": "^0.11.10",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.10.0"
  }
}
